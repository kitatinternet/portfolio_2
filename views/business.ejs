<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>

<!-- Main Content -->
<main class="container">
    <div class="row">
        <div class="col-md-offset-3 col-md-10">
            <h1><%= title %></h1>
            <br>
            <table class="table table-bordered table-striped table-hover">
                <!-- header row-->
                <tr class="d-flex">                    
                    <th class="text-center col-4">Contact Name</th>
                    <th class="text-center col-3">Contact Number</th>
                    <th class="text-center col-4">Email Address</th>
                </tr>

                <!-- repeatable temple row-->
                <% for(let count = 0; count < ContectList.length; count++) { %>
                    <tr class="d-flex">
                        <td class="text-center col-4"><%= ContectList[count].name %></td>
                        <td class="text-center col-3"><%= ContectList[count].number %></td>
                        <td class="text-center col-4"><%= ContectList[count].email %></td>
                    </tr>
                <% } %>
            </table>

        </div>
    </div>
</main>

<%- include partials/bottom_nav.ejs %>

<%- include partials/footer.ejs %>